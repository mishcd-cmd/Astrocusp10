{
  "expo": {
    "name": "Astrocusp",
    "slug": "astro-cusp",
    "scheme": "astrocusp",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1121"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0f1121"
      }
    },
    "ios": {
      "icon": "./assets/images/icon.png",
      "bundleIdentifier": "com.mutantmish.astrocusp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/images/icon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-iap"],
    "extra": {
      "eas": {
        "projectId": "7d4214fb-69e2-4174-84aa-ef4ffd7242e3"
      }
    }
  }
}
